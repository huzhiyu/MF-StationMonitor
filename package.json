{
  "name": "mf-stationmonitor",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint --fix --ext .js,.vue,jsx,ts,tsx src",
    "format": "prettier --write src/",
    "lint:style": "stylelint **/*.{html,vue,css,sass,scss} --fix",
    "prettier:format": "prettier --config .prettierrc.cjs  --ext .js,.vue,jsx,ts,tsx src --write"
  },
  "dependencies": {
    "@tweenjs/tween.js": "19.0.0",
    "animate.css": "4.1.1",
    "autofit.js": "3.2.8",
    "echarts": "5.6.0",
    "gsap": "3.13.0",
    "lodash": "4.17.21",
    "three": "0.181.0",
    "vue": "3.5.22"
  },
  "devDependencies": {
    "@tsconfig/node18": "18.2.4",
    "@types/lodash": "4.17.20",
    "@types/node": "18.19.130",
    "@typescript-eslint/eslint-plugin": "8.46.3",
    "@vitejs/plugin-vue": "4.6.2",
    "@vitejs/plugin-vue-jsx": "3.1.0",
    "@vue/tsconfig": "0.8.1",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.10.2",
    "eslint-import-resolver-typescript": "3.10.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "4.2.5",
    "eslint-plugin-vue": "9.33.0",
    "npm-run-all2": "6.2.6",
    "postcss": "8.5.6",
    "postcss-html": "1.8.0",
    "prettier": "2.8.8",
    "rollup-plugin-visualizer": "5.14.0",
    "sass": "1.93.3",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recess-order": "3.1.0",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-config-standard": "29.0.0",
    "stylelint-config-standard-scss": "6.1.0",
    "typescript": "5.9.3",
    "vite": "5.4.21",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-imagemin": "0.6.1",
    "vite-plugin-stylelint": "4.3.0",
    "vue-tsc": "1.8.27"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "yarn lint",
      "yarn prettier:format"
    ],
    "*.{html,css,sass,scss,vue}": [
      "yarn lint:style"
    ]
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  }
}
